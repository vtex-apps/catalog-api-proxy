[
  {
    "name": "catalog-proxy",
    "description": "Allows access to the catalog proxy route",
    "statements": [
      {
        "actions": ["get"],
        "effect": "allow",
        "resources": [
          "vrn:vtex.catalog-api-proxy:{{region}}:{{account}}:{{workspace}}:/proxy/catalog/*"
        ]
      }
    ]
  }
]
